{
  "name": "slackbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f env.list node index.js",
    "start:ci": "node index.js",
    "prettier": "prettier --write './**/*.js'",
    "precommit": "lint-staged",
    "docker:build": "docker build -t slack-jira-bot .",
    "docker:start": "docker run --env-file env.list --rm -p 5000:5000 -d slack-jira-bot"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "cakeinpanic",
  "license": "ISC",
  "dependencies": {
    "@slack/client": "^4.2.0",
    "chalk": "^2.4.1",
    "jira": "^0.9.2",
    "jira-rest-client": "^1.0.4",
    "lint-staged": "^7.1.0",
    "lodash": "^4.17.10",
    "prettier": "^1.12.1"
  },
  "devDependencies": {
    "env-cmd": "^9.0.3",
    "husky": "^0.14.3"
  }
}
